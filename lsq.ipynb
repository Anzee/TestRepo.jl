   "source": [
    "using Plots\n",
    "x=[0.0,2.0,4.0]\n",
    "y=[0.0,3.0,5.0]\n",
    "a0,a1=lsq_fit(x,y)\n",
    "scatter(x,y)\n",
    "plot(x,a0.+a1.*x)\n",
    "savefig(\"data.png\")"
   ]
  },
